#fusca passat dkv porsche
@declare {10.0.0.1, 10.0.0.2, 10.0.0.3, 10.0.0.4};

START:
  after (10s) do
    partition {10.0.0.1, 10.0.0.2} {10.0.0.3, 10.0.0.4};
  end
  after (20s) do
    partition {10.0.0.1, 10.0.0.2, 10.0.0.3, 10.0.0.4};
  end
  after (30s) do
    partition {10.0.0.1, 10.0.0.3} {10.0.0.2, 10.0.0.4};
  end
  after (40s) do
    partition {10.0.0.1, 10.0.0.2, 10.0.0.3, 10.0.0.4};
  end
  after (50s) do
    partition {10.0.0.2, 10.0.0.3} {10.0.0.1, 10.0.0.4};
  end

STOP:
  after (60s);
